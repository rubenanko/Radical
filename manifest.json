{
    "manifest_version": 3,
    "name": "Radical",
    "description": "Add blocking extension",
    "version": "1.0",
    "permissions": ["storage"],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "icons": {
      "128": "images/icon.png"
    },
    "action": {
      "default_popup": "popups/default/default_popup.html",
      "default_icon": "images/disabled_icon.png"
    },
    "background": {
      "service_worker": "js/service-worker.js"
    }
  }